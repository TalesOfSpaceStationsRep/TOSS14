<ui:FancyWindow xmlns="https://spacestation14.io"
                xmlns:ui="clr-namespace:Content.Client.UserInterface.Controls"
                Title="{Loc 'loaded-dice-ui-window-name'}"
                Margin="5"
                Resizable="false">
    <BoxContainer Orientation="Vertical" Margin="5" MinWidth="200">
        <Button Name="UnsetButton"
                MinSize="24 24"
                HorizontalExpand="true"
                ToggleMode="true"
                Margin="0 5"
                Text="{Loc 'loaded-dice-ui-unset-text'}" />
        <GridContainer Name="Grid" Columns="4" Margin="0 5" >
        </GridContainer>
    </BoxContainer>
</ui:FancyWindow>
